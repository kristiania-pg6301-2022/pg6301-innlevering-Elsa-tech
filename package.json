  "bugs": {
    "url": "https://github.com/kristiania-pg6301-2022/pg6301-innlevering-Elsa-tech/issues"
  },
  "homepage": "https://github.com/kristiania-pg6301-2022/pg6301-innlevering-Elsa-tech#readme",
  "devDependencies": {
    "concurrently": "^7.0.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "prettier": "^2.5.1"
  }

}
